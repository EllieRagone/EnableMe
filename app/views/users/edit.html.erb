<% provide(:title, "Edit Profile") %>
<h1>Update your profile</h1>
<div class="row">
  <div class="span4 offset2 signup">
    <%= form_for(@user) do |f| %>
      <%= render 'shared/signup_form', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>